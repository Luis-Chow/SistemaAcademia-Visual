    ],
    "from": "persona p",
    "joins": [
      {
        "table": "tipo_persona tp",
        "on": "p.id_tipo_persona = tp.id"
      },
      {
        "table": "curso c",
        "on": "c.id_docente = p.id"
      },
      {
        "table": "materia m",
        "on": "c.id_materia = m.id"
      },
      {
        "table": "seccion s",
        "on": "c.id_seccion = s.id"
      }
    ],
    "where_optional": [
      "tp.descripcion = @tipo",
      "m.nombre = @materia",
      "s.nombre = @seccion"
    ],
    "order_by": [ "p.cedula" ]
  },

  // Usado en: MantenimientoPersonaForm
  "personas_filtradas": {
    "select": [
      "DISTINCT p.id",
      "p.cedula",
      "p.nombre",
      "p.apellido",
      "tp.descripcion AS tipo_persona"
    ],
    "from": "persona p",
    "joins": [
      {
        "table": "tipo_persona tp",
        "on": "p.id_tipo_persona = tp.id"
      },
      {
        "table": "inscripcion i",
        "on": "i.id_persona = p.id",
        "type": "left"
      },
      {
        "table": "curso c",
        "on": "i.id_curso = c.id",
        "type": "left"
      },
      {
        "table": "materia m",
        "on": "c.id_materia = m.id",
        "type": "left"
      },
      {
        "table": "seccion s",
        "on": "c.id_seccion = s.id",
        "type": "left"
      }
    ],
    "where_optional": [
      "tp.descripcion = @tipo",
      "(m.nombre = @materia OR m.nombre IS NULL)",
      "(s.nombre = @seccion OR s.nombre IS NULL)"
    ],
    "order_by": [ "p.cedula" ]
  },

  // Usado en: MantenimientoPersonaForm
  "materias_activas": {
    "select": [ "id", "nombre" ],
    "from": "materia",
    "order_by": [ "nombre" ]
  },

  // Usado en: MantenimientoPersonaForm
  "secciones_activas": {
    "select": [ "id", "nombre" ],
    "from": "seccion",
    "order_by": [ "nombre" ]
  },

  // Usado en: MantenimientoPersonaForm
  "tipos_de_persona": {
